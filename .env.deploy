# Database
DB_HOST=deploy-postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=deploy_db

# API Configuration
OLOLO_MOBILE_GATE_API_ORIGIN=http://host.docker.internal:8080

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=deploy_db


# JWT Configuration
JWT_SECRET=CHANGE_ME_IN_PRODUCTION_WITH_SECURE_SECRET_KEY
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=720h


# CORS Configuration
CORS_ALLOWED_ORIGINS=*

# Initial Admin Configuration
INIT_ADMIN_UUID=00000000-0000-0000-0000-000000000001
INIT_ADMIN=admin
INIT_ADMIN_PASSWORD=CHANGE_ME_IN_PRODUCTION

# 3rd Party Service Configuration
THIRD_PARTY_API_URL=http://host.docker.internal:3000

